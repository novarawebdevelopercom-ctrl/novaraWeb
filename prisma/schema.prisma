generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ContactRequest {
  id          String   @id @default(cuid())
  fullName    String
  email       String
  projectType String
  message     String
  createdAt   DateTime @default(now())
}